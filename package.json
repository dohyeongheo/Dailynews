{
  "name": "dailynews",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:utf8": "powershell -Command \"$OutputEncoding = [System.Text.Encoding]::UTF8; [Console]::OutputEncoding = [System.Text.Encoding]::UTF8; chcp 65001 | Out-Null; npm run dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "node scripts/analyze-bundle.js",
    "setup:encoding": "powershell -ExecutionPolicy Bypass -File scripts/setup-powershell-encoding.ps1",
    "fetch-news": "npx tsx scripts/fetch-news.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@sentry/nextjs": "^10.30.0",
    "@supabase/supabase-js": "^2.39.3",
    "@vercel/blob": "^0.25.0",
    "@vercel/speed-insights": "^1.3.1",
    "bcryptjs": "^2.4.3",
    "next": "^14.2.5",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.21.0",
    "typescript": "^5.5.4",
    "undici": "^7.16.0",
    "whatwg-fetch": "^3.6.20"
  }
}
